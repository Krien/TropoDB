      \ifx\caption@sls\@empty\else
        \caption@beginslc
        \sbox\@tempboxa{\caption@@@make{#1}{#2}}%
        \ifdim\wd\@tempboxa >\hsize
          \caption@endslc
        \else
          \caption@endslc
          \caption@esetup\caption@sls
        \fi
      \fi}{}%
    \captionsize\captionfont\strut
    \caption@@@make{#1}{#2}}%
  \caption@endhook
  \global\caption@starfalse}
\newcommand\caption@calcmargin{%
